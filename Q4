#include <iostream>
using namespace std;

int main() {
    int matrix[4][4] = {
        {1, 2, 3, 4},
        {12, 13, 14, 5},
        {11,16,15,6},
        {10,9,8,7}
    };
    for (int i = 0; i < 4; i++) {
        for (int j = 0; j < 4; j++) {
            cout << matrix[i][j] <<" " ;
        }
        cout << endl;  
}
    return 0;
}
