
#include<iostream>
using namespace std;
int main(){
    int arr[30];
    int n;
    int a;
    int max=arr[0];
    int secmax;
    int min=arr[0];
    int secmin;
    cin>>n;
    int i;
    for(i=0;i<n;i++){
        cin>>arr[i];
    }
    for(i=0;i<n;i++){
        a=arr[i];
       arr[i]=arr[n-i-1];
       arr[n-i-1]=a;
        cout<<arr[i];
}
    for(i=0;i<n;i++){
        if(arr[i]>max){
            secmax=max;
            max=arr[i];}
        else if(arr[i]>secmax&&arr[i]<max){
                secmax=arr[i];
            }
        
    }cout<<secmax;
    for(i=0;i<n;i++){
        if(arr[i]<min){
            secmin=min;
            min=arr[i];}
        else if(arr[i]<secmin&&arr[i]>min){
            secmin=arr[i];
        }
        }
    cout<<secmin;
}

