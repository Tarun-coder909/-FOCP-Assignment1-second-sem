#include <iostream>
using namespace std;
int main()
{
    string a;
    cin>>a;
    int i;
    int count=1;
    for(i=0;a[i]!='\0';i++){
        if(a[i]=='i'||a[i]=='a'||a[i]=='e'||a[i]=='o'||a[i]=='u'){
            a[i]='*';
        }
    
        for(int j=i+1;a[j]!=0;j++){
            if(j==i){
            count++;
        }
    }
        cout<<"frequency of " <<a[i] <<"="<<count<<endl;
        
    }

    return 0;
}
